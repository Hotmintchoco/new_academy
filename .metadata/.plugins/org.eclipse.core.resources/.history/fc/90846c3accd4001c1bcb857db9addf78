
class Apple2 {
	@Override
	public String toString() {
		return "apple";
	}
}

class Orange2 {
	@Override
	public String toString() {
		return "orange";
	}
}

class Box2<T> {
	private T ob;

	public T getOb() {
		return ob;
	}

	public void setOb(T ob) {
		this.ob = ob;
	}
}

public class GenericsEx2 {

	public static void main(String[] args) {
		Box2<Apple> aBox = new Box2<Apple>();
		
	}

}
